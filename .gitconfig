[user]
	#name = me
	#email = me@gmail.com

[alias]
	s = status
	st = status --short
	b = branch -a --color
	d = branch -d
	#lol = log --graph --color --oneline
	ci = commit -v -a
	c = checkout
	cb = checkout -b
	ls = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate
	lol = log --graph --abbrev-commit --decorate --date=relative --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all
	p = pull -p
	ignored = ls-files --other --ignored --exclude-standard
   m = merge --no-ff



[push]
	default = current
[gui]
	editor = sublime
	historybrowser = gitk
[cola]
	linebreak = false
